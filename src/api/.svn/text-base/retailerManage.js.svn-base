import request from '@/utils/request'
export function fetchList(params) {
  return request({
    url:'/order/retailerManageList',
    method:'get',
    params:params
  })
}

export function underList(params) {
  return request({
    url:'/order/underList',
    method:'get',
    params:params
  })
}

export function financeList(params) {
  return request({
    url:'/order/getFinanceList',
    method:'get',
    params:params
  })
}

export function getHeji(params) {
  return request({
    url:'/order/getHeji',
    method:'get',
    params:params
  })
}

export function getdkDetail(params) {
