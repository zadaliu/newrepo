import request from '@/utils/request'
export function fetchList(params) {
  return request({
    url:'/shop/transport/page',
    method:'get',
    params:params
  })
}

export function test(params) {
  return request({
    url:'/test/ok/list',
    method:'get',
    params:params
  })
}


export function fors(params) {
  return request({
    url:'/test/ok/fors',
    method:'get',
    params:params
  })
}







export function areaList(params) {
  return request({
    url:'/admin/area/list',
    method:'get',
    params:params
  })
}


export function transportInfo(id) {
  return request({
    url:'/shop/transport/info/'+id,
    method:'get',
  })
}



export function createtransport(data) {
  return request({
    url:'/shop/transport',
    method:'post',
    data:data
  })
}

export function updatetransport(data) {
  return request({
    url:'/shop/transport',
    method:'put',
    data:data
  })
}


export function deletetransport(data) {
  return request({
    url:'/shop/transport',
    method:'delete',
    data:data
  })
}


